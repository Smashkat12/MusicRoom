    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "connect-timeout": "^1.9.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "njwt": "^1.0.0",
