{
  "name": "musicroom",
  "version": "1.0.0",
  "description": "a music streaming webapp",
  "apidoc": {
    "title": "music room Api",
    "url": "http://localhost:5000"
  },
  "main": "server.js",
  "scripts": {
    "server": "nodemon -r esm server/server.js",
    "docs": "apidoc-swagger -i ./server/routes/*.js -o ./server/docs",
    "client": "cd ./client && npm run serve",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run server\""
  },
  "homepage": "",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
